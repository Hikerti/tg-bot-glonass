{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src",
  "entryFile": "main",
  "compilerOptions": {
    "assets": [],
    "watchAssets": true
  },
  "projects": {
    "bot": {
      "type": "application",
      "root": "src/bot",
      "entryFile": "main",
      "sourceRoot": "src/bot",
      "compilerOptions": {
        "outDir": "dist/bot"
      }
    },
    "gate": {
      "type": "application",
      "root": "src/gate",
      "entryFile": "main",
      "sourceRoot": "src/gate",
      "compilerOptions": {
        "outDir": "dist/gate"
      }
    }
  }
}
